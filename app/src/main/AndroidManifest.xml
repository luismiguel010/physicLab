<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.physiclab">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.sensor.proximity"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ExperimentMUAProx"></activity>
        <activity android:name=".ExperimentMURTimer" />
        <activity android:name=".ExperimentMurSound" />
        <activity android:name=".Muc_Theory" />
        <activity android:name=".MuaTheory" />
        <activity android:name=".MurTheory" />
        <activity android:name=".ProblemsMuc" />
        <activity android:name=".ProblemsMua" />
        <activity android:name=".ProblemsMur" />
        <activity android:name=".ExperimentMuc" />
        <activity android:name=".MucIntroduction" />
        <activity android:name=".ExperimentMua" />
        <activity android:name=".MuaIntroduction" />
        <activity android:name=".ExperimentsMur" />
        <activity android:name=".MurIntroduction" />
        <activity android:name=".MainActivity" />
        <activity android:name=".PreStart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>